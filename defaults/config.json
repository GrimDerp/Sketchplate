{
	"editor": "orion",
	"editors": {
		"atom": [
			"atom",
			"%path%"
		],
		"orion": [
			"node",
			"%sketchplate%orion.js"
		],
		"BBEdit (osx)": [
			"bbedit",
			"%path%"
		],
		"Sublime Text 3 (osx)": [
			"/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl",
			"%path%"
		],
		"Sublime Text 2 (osx)": [
			"/Applications/Sublime Text 2.app/Contents/SharedSupport/bin/subl",
			"%path%"
		],
		"Sublime Text - in $PATH (run `subl` process)": [
			"subl",
			"%path%"
		],
		"Textmate (osx)": [
			"mate",
			"%path%"
		],
		"Vim": [
			"vim",
			"%path%"
		],
		"Vim - in new terminal (osx)": [
			"osascript",
			"-e",
			"tell application \"Terminal\" to do script \"vim %path%\""
		],
		"WebStorm": [
			"wstorm",
			"%path%"
		]
	},
	"gitInit": {
		"addAll": true,
		"commit": true,
		"push": true,
		"commitMessage": "Initial Import",
		"remoteName": "origin",
		"branch": "develop"
	},
	"template": "amd-sketch",
	"templatesPath": "~/.sketchplate/templates"
}
